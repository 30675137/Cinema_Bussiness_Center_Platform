# Lark (Feishu) Application Credentials
# Obtain from https://open.feishu.cn/app

# Application ID (app_id)
LARK_APP_ID=cli_xxxxxxxxxxxxxxxx

# Application Secret (app_secret)
LARK_APP_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# User Access Token (optional, for user-level operations)
# If provided, will be used instead of app-level token
LARK_USER_ACCESS_TOKEN=

# Base App Configuration (set after init)
LARK_BASE_APP_TOKEN=

# Logging Level (debug, info, warn, error)
LOG_LEVEL=info

# Retry Configuration
RETRY_MAX_ATTEMPTS=3
RETRY_DELAY_MS=1000
