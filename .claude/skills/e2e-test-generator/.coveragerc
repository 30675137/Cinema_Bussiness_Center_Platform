# @spec T002-e2e-test-generator
# Coverage configuration for e2e-test-generator

[run]
source = scripts
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    scripts/cli.py
    scripts/schema_validator.py
    scripts/template_renderer.py

[report]
precision = 2
show_missing = True
skip_covered = False
fail_under = 85

[html]
directory = htmlcov
