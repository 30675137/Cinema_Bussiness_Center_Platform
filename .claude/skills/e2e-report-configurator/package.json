{
  "name": "e2e-report-configurator",
  "version": "1.0.0",
  "description": "Configure Playwright test report outputs for E2E tests",
  "type": "module",
  "main": "scripts/index.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "lint": "eslint scripts tests --ext .ts",
    "lint:fix": "eslint scripts tests --ext .ts --fix"
  },
  "keywords": [
    "playwright",
    "testing",
    "e2e",
    "reports",
    "ci-cd",
    "claude-code-skill"
  ],
  "author": "Claude Code",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.10",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
