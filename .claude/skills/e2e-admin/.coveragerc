# @spec T001-e2e-orchestrator
# Coverage configuration for e2e-admin skill

[run]
source = scripts
omit =
    # Exclude orchestrate.py (integration entry point, complex to unit test)
    scripts/orchestrate.py
    # Exclude skill_executor.py (P2 feature, not MVP requirement)
    scripts/skill_executor.py
    # Exclude __pycache__ and test files
    */__pycache__/*
    */tests/*

[report]
precision = 2
exclude_lines =
    # Standard exclusions
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    # Defensive programming
    pass
    \.\.\.

[html]
directory = htmlcov
